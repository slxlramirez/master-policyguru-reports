| dbxquery 
maxrows=250000
connection="mssv_db" 
timeout=1800 query="
SELECT   datekey,
         count(a.guid) as calls
FROM     pg_fs_pfd_cdr a 
WHERE    datekey >= '2020-02-01'::date AND
         datekey  < '2020-03-16'::date AND
         a.direction = 'INBOUND'
GROUP BY datekey
ORDER BY datekey
" shortnames=t
| table datekey calls
